{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "arrow-body-style": "off",
      "react/jsx-filename-extension": "off",
      "global-require": "off"
    },
    "globals": {
      "navigator": true,
      "fetch": true
    }
}
